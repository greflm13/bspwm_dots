*faceName:      Hack
*faceSize:      8
xterm*utf8:	1
xterm*cursorBlink: true
xterm*selectToClipboard: true
xterm*ScrollBar: false

! X colors.
! Generated by 'wal'
*foreground:        #F6F6F6
*background:        #16171d
*.foreground:       #F6F6F6
*.background:       #16171d
emacs*foreground:   #F6F6F6
emacs*background:   #16171d
URxvt*foreground:   #F6F6F6
XTerm*foreground:   #F6F6F6
UXTerm*foreground:  #F6F6F6
URxvt*background:   [100]#16171d
XTerm*background:   #16171d
UXTerm*background:  #16171d
URxvt*cursorColor:  #F6F6F6
XTerm*cursorColor:  #F6F6F6
UXTerm*cursorColor: #F6F6F6
URxvt*borderColor:  [100]#16171d

! Colors 0-15.
*.color0: #000000
*color0:  #000000
*.color1: #FF3F4F
*color1:  #FF3F4F
*.color2: #81F900
*color2:  #81F900
*.color3: #FFD945
*color3:  #FFD945
*.color4: #528bff
*color4:  #528bff
*.color5: #E542FF
*color5:  #E542FF
*.color6: #19D1E5
*color6:  #19D1E5
*.color7: #F6F6F6
*color7:  #F6F6F6
*.color8: #D7DAE0
*color8:  #D7DAE0
*.color9: #c24038
*color9:  #c24038
*.color10: #6FD80D
*color10:  #6FD80D
*.color11: #FFE792
*color11:  #FFE792
*.color12: #61afef
*color12:  #61afef
*.color13: #E373CE
*color13:  #E373CE
*.color14: #6AEBF7
*color14:  #6AEBF7
*.color15: #F8FAFD
*color15:  #F8FAFD

! Black color that will not be affected by bold highlighting.
*.color66: #16171d
*color66:  #16171d

! Xclock colors.
XClock*foreground: #F6F6F6
XClock*background: #16171d
XClock*majorColor:  rgba:bb/b4/b9/ff
XClock*minorColor:  rgba:bb/b4/b9/ff
XClock*hourColor:   rgba:bb/b4/b9/ff
XClock*minuteColor: rgba:bb/b4/b9/ff
XClock*secondColor: rgba:bb/b4/b9/ff

! Set depth to make transparency work.
URxvt*depth: 32
